<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	
  <TableSelectDialog id="DialogPtoExp" 
                       noDataText="No se ha encontrado datos" 
                       title="{i18n>TitleF4PtoExp}" 
                       search=".handleSearchPtoExp" 
                       confirm=".handleCloseHpPtoExp" 
                       cancel=".handleCloseHpPtoExp" 
                       items="{ path : '/HTvstSet' }" 
                       contentHeight="100%"
					   contentWidth="50%">

        <ColumnListItem>
            <cells>
                <Text text="{Vstel}" />
                <Text text="{Vtext}"/>
            </cells>
        </ColumnListItem>

        <columns>
            <Column minScreenWidth="Tablet" demandPopin="true" width="200px">
                <header>
                    <Text text="{i18n>IdVstel}" />
                </header>
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <header>
                     <Text text="{i18n>IdVtext}" />
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>