<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <TableSelectDialog id="DialogUMDetail" 
                       noDataText="No se ha encontrado datos" 
                       title="{i18n>TitleF4UMDetail}" 
                       search=".handleSearchUMDetail" 
                       confirm=".handleCloseHpUMDetail" 
                       cancel=".handleCloseHpUMDetail" 
                       items="{ path : '/ZshMeinsGreSet' }" 
                       contentHeight="100%"
					   contentWidth="50%">
        <ColumnListItem>
            <cells>
                <Text text="{Msehi}" />
                <Text text="{Msehl}"/>
            </cells>
        </ColumnListItem>
        <columns>
            <Column minScreenWidth="Tablet" demandPopin="true" width="200px">
                <header>
                    <Text text="{i18n>IdMeins}" />
                </header>
            </Column>
            <Column minScreenWidth="Tablet" demandPopin="true">
                <header>
                     <Text text="{i18n>IdMeinsDesc}" />
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>