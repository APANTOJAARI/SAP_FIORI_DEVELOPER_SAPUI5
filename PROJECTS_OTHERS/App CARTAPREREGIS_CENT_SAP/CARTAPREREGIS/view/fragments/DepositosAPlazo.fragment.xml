<core:FragmentDefinition xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns="sap.m"
	xmlns:core="sap.ui.core" height="100%" xmlns:l="sap.ui.layout" controllerName="com.centria.CartaPreregistro.controller.View1">
	<f:Form editable="true">
		<f:toolbar>
			<Toolbar>
				<Title text="{i18n>titleDepositosPlazo}"/>
				<ToolbarSpacer/>
			</Toolbar>
		</f:toolbar>
		<f:layout>
			<f:ResponsiveGridLayout labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
				emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="1" singleContainerFullSize="false"/>
		</f:layout>
		<f:formContainers>
			<f:FormContainer>
				<f:formElements>
					<f:FormElement label="{i18n>inputSociedad}">
						<Input id="idInputSociedad1" suggestionItems="{ path: 'ServiceH2HModel>/SociedadSet',length: 10000 }"
							valueHelpRequest="openMatchCodeSociedad" showValueHelp="true" class="UpperCase" showSuggestion="true"
							selectedKey="{oModelDataGlobal>/InpSociedad}" value="{oModelDataGlobal>/InpSociedad}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
							<core:ListItem key="{ServiceH2HModel>Companycode}" text="{ServiceH2HModel>Companycode}" additionalText="{ServiceH2HModel>Companycodename}"/>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputRuc}">
						<Input value="{oModelDataGlobal>/InpRuc}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputAtte}">
						<Input value="{oModelDataGlobal>/InpAtte}">
							<layoutData>
								<l:GridData span="L9 M9 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputImport}">
						<Input value="{oModelDataGlobal>/InpImporte}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputFecReg}">
						<DatePicker placeholder="Fecha" dateValue="{oModelDataGlobal>/InpFecRegistro}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</DatePicker>
					</f:FormElement>
					<f:FormElement label="{i18n>inputFecCont}">
						<DatePicker placeholder="Fecha" dateValue="{oModelDataGlobal>/InpFecConta}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</DatePicker>
					</f:FormElement>
					<f:FormElement label="{i18n>inputClaBanC}">
						<MultiInput id="idInputClaveBanco" value="{oModelDataGlobal>/inputClaBanCargar}" showValueHelp="true">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</MultiInput>
					</f:FormElement>
					<f:FormElement label="{i18n>inputCuentaC}">
						<Input value="{oModelDataGlobal>/InpCuentaCargo}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputClaBanA}">
						<Input showValueHelp="true" value="{oModelDataGlobal>/inputClaBanAbonar}">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputCuentaA}">
						<Input value="{oModelDataGlobal>/inputCuentaAbono}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputCuentaDepPla}">
						<Input showValueHelp="true">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputFecApe}">
						<DatePicker placeholder="Fecha" dateValue="{oModelDataGlobal>/inputFecApe}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</DatePicker>
					</f:FormElement>
					<f:FormElement label="{i18n>inputPlazo}">
						<Input value="{oModelDataGlobal>/inputPlazo}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputFecVen}">
						<DatePicker placeholder="Fecha" dateValue="{oModelDataGlobal>/inputFecVen}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</DatePicker>
					</f:FormElement>
					<f:FormElement label="{i18n>inputTasa}">
						<Input value="{oModelDataGlobal>/inputTasa}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
					<f:FormElement label="{i18n>inputIntereses}">
						<Input value="{oModelDataGlobal>/inputIntereses}">
							<layoutData>
								<l:GridData span="L4 M4 S12"/>
							</layoutData>
						</Input>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
		</f:formContainers>
	</f:Form>
</core:FragmentDefinition>