<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<TableSelectDialog   noDataText="No se encontraron sociedades" title="Seleccione una sociedad" search=".onSociedadSearch"
		confirm=".onSociedadValueHelpOkPress" cancel=".onSociedadValueHelpOkPress" items="{ path : 'ServiceH2HModel>/SociedadSet' }"
		contentWidth="50%" contentHeight="50%" growing="true" growingThreshold="20" class="sapUiSizeCompact">
		<ColumnListItem>
			<cells>
				<Text text="{ServiceH2HModel>Companycode}"/>
				<Text text="{ServiceH2HModel>Companycodename}"/>
				<Text text="{ServiceH2HModel>Controllingarea}"/>
				<Text text="{ServiceH2HModel>Country}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Sociedad"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" width="25em">
				<header>
					<Text text="Nombre empresa"/>
				</header>
			</Column>
		
			<Column minScreenWidth="Tablet" demandPopin="true" width="12em">
				<header>
					<Text text="Sociedad CO"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" width="12em">
				<header>
					<Text text="País"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>
