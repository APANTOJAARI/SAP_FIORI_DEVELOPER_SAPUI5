<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <VBox>
        <Panel width="auto" class="sapUiResponsiveMargin">
            <f:Form>
                <f:layout>
                    <f:ResponsiveGridLayout
                        labelSpanXL="4"
                        labelSpanL="4"
                        labelSpanM="12"
                        labelSpanS="12"
                        adjustLabelSpan="false"
                        emptySpanXL="0"
                        emptySpanL="0"
                        emptySpanM="0"
                        emptySpanS="0"
                        columnsXL="2"
                        columnsL="2"
                        columnsM="2"
                        singleContainerFullSize="false" />
                </f:layout>
                <f:formContainers>

                    <f:FormContainer>
                        <f:formElements>

                            <f:FormElement label="{i18n>gerente}">
                                <f:fields>
                                    <Input id="txtGerente" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/GeGeneral}" required="true" change="changeValueState" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>gerente1}">
                                <f:fields>
                                    <Input id="txtGerente1" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/GerenteJeVn}" required="true" change="changeValueState" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>gerente2}">
                                <f:fields>
                                    <Input id="txtGerente2" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/GerenteAdm}" change="changeValueState" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>gerente3}">
                                <f:fields>
                                    <Input id="txtGerente3" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/GerenteJefCo}" change="changeValueState" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>gerente4}">
                                <f:fields>
                                    <Input id="txtGerente4" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/GeJefOpe}" change="changeValueState" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>gerente5}">
                                <f:fields>
                                    <Input id="txtGerente5" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/ContGeneral}" change="changeValueState" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>gerente6}">
                                <f:fields>
                                    <Input id="txtGerente6" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/JefeTesoreria}" change="changeValueState" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>gerente7}">
                                <f:fields>
                                    <Input id="txtGerente7" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/RespPagos}" required="true" change="changeValueState" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>

                        </f:formElements>
                    </f:FormContainer>

                    <f:FormContainer >
                        <f:formElements>

                            <f:FormElement label="{i18n>email2}">
                                <f:fields>
                                    <Input id="txtEmail2" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/EmailGe}" required="true" change="onValidarFormatoCorreo" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>email3}">
                                <f:fields>
                                    <Input id="txtEmail3" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/EmailJeVn}" required="true" change="onValidarFormatoCorreo" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>email4}">
                                <f:fields>
                                    <Input id="txtEmail4" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/EmailGeAdm}" change="onValidarFormatoCorreo" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>email5}">
                                <f:fields>
                                    <Input id="txtEmail5" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/EmailGeJc}" change="onValidarFormatoCorreo" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>email6}">
                                <f:fields>
                                    <Input id="txtEmail6" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/EmailJefOpe}" change="onValidarFormatoCorreo" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>email7}">
                                <f:fields>
                                    <Input id="txtEmail7" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/EmailContGen}" change="onValidarFormatoCorreo" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>email8}">
                                <f:fields>
                                    <Input id="txtEmail8" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/EmailJefTes}" change="onValidarFormatoCorreo" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="{i18n>email9}">
                                <f:fields>
                                    <Input id="txtEmail9" value="{oProveedor>/Ejecutivos/RegistroEjecEmpresaLogSet/0/EmailRespPa}" required="true" change="onValidarFormatoCorreo" editable="{statusEdit>/edit}"/>
                                </f:fields>
                            </f:FormElement>

                        </f:formElements>
                    </f:FormContainer>
                </f:formContainers>

            </f:Form>

            <FlexBox justifyContent="End" class="sapUiTinyMarginBeginEnd">   
                <Button type="Accept"
                        text="{i18n>grabaryseguir}"
                        width="20em"
                        press="onSaveEjecutivoEmpresa"
                        ariaDescribedBy="acceptButtonDescription genericButtonDescription"
                        id="BtnEjecutivoEmpresa" visible="{statusEdit>/edit}">
                </Button>
            </FlexBox>
        </Panel>       
    </VBox>
    
</core:FragmentDefinition>